{
  "name": "lib.cxx.tiny-dnn",
  "version": "1.0.0",
  "author": "",
  "url": "",
  "dependencies": {
    "lang.cxx": "*"
  },
  "options": {
    "version": {
      "type": "string",
      "default": "v1.0.0a3"
    },
    "directory": {
      "type": "path"
    },
    "build_examples": {
      "type": "bool"
    }
  },
  "loaders": [
    {
      "name": "source",
      "type": "url",
      "urls": [
        "file://$directory",
        "https://github.com/tiny-dnn/tiny-dnn/archive/$version.zip"
      ]
    }
  ]
}
